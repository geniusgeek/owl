{
  "mcpServers": {
    "mongodb-lens": {
      "command": "npx",
      "args": [
        "-y",
        "mongodb-lens@latest",
        "mongodb://localhost:27017/"
      ]
    },
    "postgres": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "postgresql://postgres:postgres@localhost:5432/convert"
      ]
    }
  }
}

